{
  "name": "CHYETH-PLATFORM",
  "version": "1.0.0",
  "description": "知氏课程与实践资源平台",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ CHYETH PLATFORM --all --icon=./icon.png",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "知氏课程与实践资源平台",
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type",
      "icon": "./icon.png"
    },
    "dmg": {
    },
    "win": {
      "target": "appx",
      "icon": "./icon.png"
    },
    "appx": {
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-builder": "^20.19.2",
    "electron-packager": "^12.1.0"
  }
}
