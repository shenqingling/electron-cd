{
  "name": "CHYETH-PLATFORM",
  "version": "1.0.0",
  "description": "知氏课程与实践资源平台",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ CHYETH PLATFORM --all --icon=./icon.png",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "知氏课程与实践资源平台",
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type",
      "icon": "./icon.png"
    },
    "dmg": {},
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        }
      ],
      "icon": "./icon.png"
    },
    "appx": {}
  },
  "repository": "",
  "keywords": ["chyeth", "electron"],
  "author": "知式教育",
  "license": "",
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-builder": "^20.19.2",
    "electron-packager": "^12.1.0",
    "path": "^0.12.7"
  }
}
