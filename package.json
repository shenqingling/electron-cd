{
  "name": "CHYETH",
  "version": "2.8.0-course",
  "description": "创新课程与实践资源平台",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package_no_use": "electron-packager ./ CHYETH --mac --icon=./icon.png",
    "dist_mac": "electron-builder --platform=mac",
    "dist_win": "electron-builder --platform=win"
  },
  "build": {
    "productName": "创新课程与实践资源平台",
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type",
      "icon": "./icon.png"
    },
    "dmg": {},
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./icon.png"
    },
    "appx": {}
  },
  "repository": "",
  "keywords": [
    "chyeth",
    "teacher",
    "electron"
  ],
  "author": "chyeth",
  "license": "",
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-builder": "^20.19.2",
    "electron-packager": "^12.1.0",
    "path": "^0.12.7"
  }
}
