{
  "name": "CHYETH",
  "version": "1.0.0",
  "description": "知氏老师",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package_no_use": "electron-packager ./ CHYETH --mac --icon=./icon.png",
    "dist": "electron-builder --platform=mac"
  },
  "build": {
    "productName": "知氏老师",
    "appId": "your.id",
    "mac": {
      "category": "your.app.category.type",
      "icon": "./icon.png"
    },
    "dmg": {},
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./icon.png"
    },
    "appx": {}
  },
  "repository": "",
  "keywords": [
    "chyeth",
    "teacher",
    "electron"
  ],
  "author": "知式",
  "license": "",
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-builder": "^20.19.2",
    "electron-packager": "^12.1.0",
    "path": "^0.12.7"
  }
}
